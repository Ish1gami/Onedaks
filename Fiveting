public class Fiveting
{
   public static void main(String[] args) {
       
       int [] arr = {2, 1, 9, 6, 7};
       int num = 2;
       
       for (int i = 0; i < arr.length; i++) {
           if(arr[i] == num) {
               System.out.println("Found "+ num + " at "+ i + " index ");
               break;
           }         
       }
   }
}
