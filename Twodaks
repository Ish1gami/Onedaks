public class twodags
{
     public static void main(String [] args) {
         int[] arr ={9, 3, 7, 3, 2, 4, 7, 7, 1, 7};
         int count = 0;
         
         for (int i = 0; i < arr.length; i++) {
             if ( arr[i] == 7) count++;
         }
         System.out.print("7 appears: " + count + " times");
    }
}
